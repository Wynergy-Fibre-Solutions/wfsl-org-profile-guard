{
  "name": "wfsl-org-profile-guard",
  "version": "0.3.1",
  "description": "WFSL organisation profile validation and governance guard",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "verify": "node dist/index.js --verify evidence/org-profile-check.json",
    "anchor": "node dist/index.js --anchor evidence/org-profile-check.json --anchor-log evidence/ANCHOR_LOG.ndjson",
    "verify:log": "node dist/index.js --verify-log evidence/ANCHOR_LOG.ndjson",
    "manifest": "node dist/index.js --manifest evidence/ANCHOR_MANIFEST.json",
    "witness": "node dist/index.js --manifest evidence/ANCHOR_MANIFEST.json --witness",
    "seal": "npm run anchor && npm run verify:log && npm run witness"
  },
  "dependencies": {
    "wfsl-control-plane": "github:Wynergy-Fibre-Solutions/wfsl-control-plane#v1.1.3"
  },
  "author": "Wynergy Fibre Solutions Ltd",
  "license": "ISC",
  "type": "commonjs"
}
